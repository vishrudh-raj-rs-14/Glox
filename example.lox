fun isSquareRoot(n) {
  var i = 1;
  while (i * i <= n) {
    if (i * i == n) return 1;
    i = i + 1;
  }
  return 0;
}

fun isPrime(n) {
  if (n <= 1) return 0;
  if (n <= 3) return 1;
  if (n % 2 == 0) return 0;
  
  var i = 3;
  while (i * i <= n) {
    if (n % i == 0) return 0;
    i = i + 2;
  }
  return 1;
}

var num = 2;
var count = 0;
while (count < 100) {
  if (isPrime(num)) {
    print num;
    count = count + 1;
  }
  num = num + 1;
}